{
  "name": "docs",
  "version": "0.0.0",
  "private": true,
  "description": "Small description for docs goes here",
  "repository": "",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "start": "concurrently 'npm:ember:watch' 'npm:tailwind:watch' 'npm:docs:watch'",
    "build": "yarn docs:collect && yarn tailwind:build && ember build --environment=production",
    "docs:collect": "typedoc --options ./typedoc.config.json ../ember-resources/src/index.ts",
    "docs:watch": "yarn docs:collect --watch",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "ember:watch": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "tailwind:build": "npx tailwindcss -c ./config/tailwind/tailwind.config.js -i ./config/tailwind/tailwind-input.css -o ./public/assets/tailwind.css",
    "tailwind:watch": "npx tailwindcss -c ./config/tailwind/tailwind.config.js -i ./config/tailwind/tailwind-input.css -o ./public/assets/tailwind.css --watch"
  },
  "devDependencies": {
    "@docfy/core": "^0.5.0",
    "@docfy/ember": "^0.5.0",
    "@docfy/plugin-with-prose": "^0.5.0",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.6.0",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "@tailwindcss/typography": "^0.5.1",
    "autoprefixer": "^10.0.0",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^7.0.0",
    "docfy-theme-ember": "josemarluedke/docfy-theme-ember",
    "ember-auto-import": "^2.2.4",
    "ember-cli": "~4.1.1",
    "ember-cli-app-version": "^5.0.0",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-htmlbars": "^6.0.1",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-data": "~4.1.0",
    "ember-export-application-global": "^2.0.1",
    "ember-fetch": "^8.1.1",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.3",
    "ember-source": "~4.1.0",
    "ember-template-lint": "^3.15.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-qunit": "^7.2.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.0.0",
    "prettier": "^2.5.1",
    "qunit": "^2.17.2",
    "qunit-dom": "^2.0.0",
    "rehype-highlight": "^4.0.0",
    "remark-autolink-headings": "^5.0.0",
    "remark-code-titles": "^0.1.2",
    "tailwindcss": "^3.0.0",
    "typedoc": "^0.22.0",
    "typedoc-github-wiki-theme": "^1.0.0",
    "typedoc-gitlab-wiki-theme": "^1.0.0",
    "typedoc-plugin-markdown": "^3.11.11",
    "typedoc-plugin-markdown-pages": "^0.3.0",
    "typedoc-plugin-mdn-links": "^1.0.4",
    "typedoc-plugin-merge-modules": "^3.1.0",
    "typescript": "4.5.5",
    "webpack": "^5.65.0"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  }
}
